{
  "name": "bjs-game01",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "jest"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect",
      "jest-canvas-mock",
      "jest-extended",
      "<rootDir>/jest.setup.ts"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@types/cannon": "^0.1.8",
    "@types/jest": "^29.5.0",
    "@types/randomcolor": "^0.5.7",
    "jest": "^29.5.0",
    "jest-canvas-mock": "^2.5.0",
    "jest-extended": "^3.2.4",
    "ts-jest": "^29.1.0",
    "typescript": "^5",
    "vite": "^4.2.0"
  },
  "dependencies": {
    "@babylonjs/core": "^5.54.0",
    "@babylonjs/gui": "^5.54.0",
    "@babylonjs/gui-editor": "^5.54.0",
    "@babylonjs/inspector": "^5.54.0",
    "@babylonjs/loaders": "^5.54.0",
    "@babylonjs/materials": "^5.54.0",
    "@babylonjs/serializers": "^5.54.0",
    "cannon": "^0.6.2",
    "randomcolor": "^0.6.2"
  }
